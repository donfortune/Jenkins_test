pipeline {
    agent any
    stages {
        stage("Git checkout") {
            steps {
                script {
                    checkout([$class: 'GitSCM', branches: [[name: '*/main']], userRemoteConfigs: [[url: 'https://github.com/donfortune/Jenkins_test.git']]])
                }
            }
        }
        stage("Build image") {
            steps {
                script {
                    sh "docker build -t myapp3 ."
                }
            }
        }
    }
}
